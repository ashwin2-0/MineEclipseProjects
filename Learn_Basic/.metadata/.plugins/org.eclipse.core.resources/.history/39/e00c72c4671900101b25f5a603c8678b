package src.com.supplier;

import java.util.Arrays;
import java.util.HashMap;
import java.util.List;
import java.util.Map;
import java.util.function.Supplier;

public class SupplierDemoLatest {
	public static void main(String[] args) {
		Supplier<String> op = () -> "Hello Java";
		System.out.println(op.get());
		Supplier<String> msg = () -> "no found";
		//List<String> list = Arrays.asList("a", "b", "c");
		List<String> list = Arrays.asList();
		System.out.println(list.stream().findAny().orElseGet(msg));
		Map<Integer, String> map = new HashMap<>();

		// Adding 20 entries to the map
		map.put(1, "Echo");
		map.put(2, "Shadow");
		map.put(3, "Galaxy");
		map.put(4, "Thunder");
		map.put(5, "Mystic");
		map.put(6, "Blaze");
		map.put(7, "Aurora");
		map.put(8, "Solstice");
		map.put(9, "Nebula");
		map.put(10, "Zenith");
		map.put(11, "Eclipse");
		map.put(12, "Comet");
		map.put(13, "Vortex");
		map.put(14, "Cipher");
		map.put(15, "Phantom");
		map.put(16, "Quasar");
		map.put(17, "Vertex");
		map.put(18, "Radiant");
		map.put(19, "Spectrum");
		map.put(20, "Momentum");
		map.entrySet().forEach(System.out::p[rintln);
	}

}
