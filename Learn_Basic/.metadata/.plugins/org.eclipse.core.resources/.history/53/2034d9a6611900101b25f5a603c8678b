package src.com.predicate;

import java.util.function.Predicate;

public class PredicateDemoTraditional {

	Predicate<Integer> predicate = (Integer t) -> {
		if (t % 2 == 0) {
			return true;
		} else {
			return false;
		}
	};

	public static void main(String[] args) {
		System.out.println(obj.test(44));

	}

}
